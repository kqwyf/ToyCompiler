int print() {}

int min(int a, int b) {
    if(a < b)
        return a;
    else
        return b;
}

int gcd(int a, int b) {
    if(b == 0)
        return a;
    else
        return gcd(b, a - a / b * b);
}

int main() {
    int a, b;
    a = input();
    b = input();
    print(gcd(a, b));
}
